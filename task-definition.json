{
    "family": "laravel-backend-app",
    "containerDefinitions": [
        {
            "name": "nginx",
            "image": "public.ecr.aws/docker/library/httpd:latest",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "laravelapp-nginx",
                    "awslogs-region": "ap-southeast-1",
                    "awslogs-stream-prefix": "laravel-backend-app"
                }
            },
            "dependsOn": [
                {
                    "containerName": "php-fpm",
                    "condition": "START"
                }
            ],
            "essential": true
        },
        {
            "name": "php-fpm",
            "image": "public.ecr.aws/docker/library/httpd:latest",
            "linuxParameters": {
                "initProcessEnabled": true
            },
            "portMappings": [
                {
                    "containerPort": 9000
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "laravelapp-php-fpm",
                    "awslogs-region": "ap-southeast-1",
                    "awslogs-stream-prefix": "laravel-backend-app"
                }
            },
            "essential": true
        }
    ],
    "taskRoleArn": "ecsTaskRoleMinh",
    "executionRoleArn": "ecsTaskExecutionRoleMinh",
    "cpu": "256",
    "memory": "512",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ]
}